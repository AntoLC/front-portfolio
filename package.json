{
  "name": "front-portfolio",
  "version": "1.0.0",
  "homepage": "https://antolc.github.io/front-portfolio/",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.4.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "gh-pages": "^3.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "5",
    "react-scripts": "4.0.3",
    "recoil": "^0.5.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.1.2",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "testCoverage": "craco test --coverage",
    "eject": "react-scripts eject",
    "deploy": "git push origin main",
    "deploy-web": "gh-pages -d build",
    "my-prettier-sm": "yarn prettier --write --print-width 100 ./src/**/*.{js,ts,tsx}",
    "my-prettier-m": "yarn prettier --write --print-width 160 ./src/**/*.{js,ts,tsx}",
    "my-prettier-l": "yarn prettier --write --print-width 200 ./src/**/*.{js,ts,tsx}",
    "upgrade-packages": "yarn upgrade-interactive --latest",
    "gitAdd": "yarn lint && yarn my-prettier-m && git add .",
    "lint": "eslint src --ext .js,.ts,.tsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "parser": "@typescript-eslint/parser"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.3.2",
    "autoprefixer": "9.0.0",
    "node-sass": "6.0.1",
    "postcss": "8.3.11",
    "prettier": "^2.4.1",
    "sass-loader": "12.1.0"
  }
}
